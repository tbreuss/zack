{
  "name": "tebe/zack",
  "description": "Zack! is a tiny little microframework based on the HttpKernel Component of Symfony and uses file-based routing as its main feature.",
  "type": "library",
  "require": {
    "league/commonmark": "^2.6",
    "symfony/dependency-injection": "^7.2",
    "symfony/event-dispatcher": "^7.2",
    "symfony/finder": "^7.2",
    "symfony/http-foundation": "^7.2",
    "symfony/http-kernel": "^7.2",
    "symfony/routing": "^7.2",
    "twig/twig": "^3.20"
  },
  "autoload": {
    "psr-4": {
      "tebe\\zack\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Thomas Breuss"
    }
  ],
  "minimum-stability": "stable",
  "license": "MIT",
  "config": {
    "sort-packages": true
  }
}
